    private void jComboBox11ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        if(jComboBox11.getSelectedItem()=="Bar1"){
            int idx1=Alcohol.getSelectedIndex();
            String alcoholtype=alcohol[idx1];

            String stralcoholposotita=String.valueOf(jSpinner5.getValue());

            alcoholtimi1=alcoholprice[idx1];
            alcoholposotita1=Integer.valueOf(stralcoholposotita);
            Object[] row6 ={alcoholtype, alcoholposotita1, alcoholtimi1, alcoholtimi1*alcoholposotita1};
            DefaultTableModel model6= (DefaultTableModel) jTable3.getModel();
            model6.addRow(row6);

          

           sumpa1=sumpa1+alcoholtimi1*alcoholposotita1;

            sunoloal1 = sumpc1+sumpa1+sumpb1+sumpan1+sumpx1;

            stimi1.setText(String.valueOf(sunoloal1));

            Alcohol.clearSelection();

        }
        if(jComboBox11.getSelectedItem()=="Bar2"){
            int idx2=Alcohol.getSelectedIndex();
            String alcoholtype=alcohol[idx2];
            String stralcoholposotita=String.valueOf(jSpinner5.getValue());
            alcoholtimi2=alcoholprice[idx2];
            alcoholposotita2=Integer.valueOf(stralcoholposotita);
            Object[] row6 ={alcoholtype, alcoholposotita2, alcoholtimi2, alcoholtimi2*alcoholposotita2};
            DefaultTableModel model6= (DefaultTableModel) jTable4.getModel();
            model6.addRow(row6);
            
 sumpa2=sumpa2+alcoholtimi2*alcoholposotita2;

            sunoloal2 = sumpc2+sumpa2+sumpb2+sumpan2+sumpx2;

           // sunoloal2 = sunoloal2+ (cafetimi2*cafeposotita2)+(xumoitimi2*xumoiposotita2)+(anapsiktikatimi2*anapsiktikaposotita2)+(beertimi2*beerposotita2)+(alcoholtimi2*alcoholposotita2);
            stimi2.setText(String.valueOf(sunoloal2));

            Alcohol.clearSelection();}
        if(jComboBox11.getSelectedItem()=="Bar3"){
            int idx3=Alcohol.getSelectedIndex();
            String alcoholtype=alcohol[idx3];
            String stralcoholposotita=String.valueOf(jSpinner5.getValue());
            alcoholtimi3=alcoholprice[idx3];
            alcoholposotita3=Integer.valueOf(stralcoholposotita);
            Object[] row6 ={alcoholtype, alcoholposotita3, alcoholtimi3, alcoholtimi3*alcoholposotita3, alcoholtimi3*alcoholposotita3};
            DefaultTableModel model6= (DefaultTableModel) jTable5.getModel();
            model6.addRow(row6);
             sumpa3=sumpa3+alcoholtimi3*alcoholposotita3;

            sunoloal3 = sumpc3+sumpa3+sumpb3+sumpan3+sumpx3;

            //sunoloal3 = sunoloal3+(cafetimi3*cafeposotita3)+(xumoitimi3*xumoiposotita3)+(anapsiktikatimi3*anapsiktikaposotita3)+(beertimi3*beerposotita3)+(alcoholtimi3*alcoholposotita3);//+(cafetimi4*cafeposotita4)+(xumoitimi4*xumoiposotita4)+(anapsiktikatimi4*anapsiktikaposotita4)+(beertimi4*beerposotita4)+(alcoholtimi4*alcoholposotita4);
            stimi3.setText(String.valueOf(sunoloal3));
            

            Alcohol.clearSelection();

        }
        if(jComboBox11.getSelectedItem()=="Bar4"){
            int idx3=Alcohol.getSelectedIndex();
            String alcoholtype=alcohol[idx3];
            String stralcoholposotita=String.valueOf(jSpinner5.getValue());
            alcoholtimi4=alcoholprice[idx3];
            alcoholposotita4=Integer.valueOf(stralcoholposotita);
            Object[] row6 ={alcoholtype, alcoholposotita4, alcoholtimi4, alcoholtimi4*alcoholposotita4};
            DefaultTableModel model6= (DefaultTableModel) jTable6.getModel();
            model6.addRow(row6);
 sumpa4=sumpa4+alcoholtimi4*alcoholposotita4;

            sunoloal4 = sumpc4+sumpa4+sumpb4+sumpan4+sumpx4;

            //sunoloal4 =sunoloal4+(cafetimi4*cafeposotita4)+(xumoitimi4*xumoiposotita4)+(anapsiktikatimi4*anapsiktikaposotita4)+(beertimi4*beerposotita4)+(alcoholtimi4*alcoholposotita4);//+ (cafetimim1*cafeposotitam1)+(xumoitimim1*xumoiposotitam1)+(anapsiktikatimim1*anapsiktikaposotitam1)+(beertimim1*beerposotitam1)+(alcoholtimim1*alcoholposotitam1)+(cafetimim2*cafeposotitam2)+(xumoitimim2*xumoiposotitam2)+(anapsiktikatimim2*anapsiktikaposotitam2)+(beertimim2*beerposotitam2)+(alcoholtimim2*alcoholposotitam2);
            stimi4.setText(String.valueOf(sunoloal4));

            Alcohol.clearSelection();
        }
        if(jComboBox11.getSelectedItem()=="Mesa1"){
            int idx3=Alcohol.getSelectedIndex();
            String alcoholtype=alcohol[idx3];
            String stralcoholposotita=String.valueOf(jSpinner5.getValue());
            alcoholtimim1=alcoholprice[idx3];
            alcoholposotitam1=Integer.valueOf(stralcoholposotita);
            Object[] row6 ={alcoholtype, alcoholposotitam1, alcoholtimim1, alcoholtimim1*alcoholposotitam1};
            DefaultTableModel model6= (DefaultTableModel) jTable7.getModel();
            model6.addRow(row6);
 sumpa5=sumpa5+alcoholtimim1*alcoholposotitam1;

            sunoloal5 = sumpc5+sumpa5+sumpb5+sumpan5+sumpx5;

            //sunoloal5 =sunoloal5+ (cafetimim1*cafeposotitam1)+(xumoitimim1*xumoiposotitam1)+(anapsiktikatimim1*anapsiktikaposotitam1)+(beertimim1*beerposotitam1)+(alcoholtimim1*alcoholposotitam1);
            stimi5.setText(String.valueOf(sunoloal5));

            Alcohol.clearSelection();
        }
        if(jComboBox11.getSelectedItem()=="Mesa2"){
            int idx3=Alcohol.getSelectedIndex();
            String alcoholtype=alcohol[idx3];
            String stralcoholposotita=String.valueOf(jSpinner5.getValue());
            alcoholtimim2=alcoholprice[idx3];
            alcoholposotitam2=Integer.valueOf(stralcoholposotita);
            Object[] row6 ={alcoholtype, alcoholposotitam2, alcoholtimim2, alcoholtimim2*alcoholposotitam2};
            DefaultTableModel model6= (DefaultTableModel) jTable8.getModel();
            model6.addRow(row6);
            
